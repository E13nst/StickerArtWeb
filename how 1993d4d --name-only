  setAuthHeaders(initData: string) {
    this.client.defaults.headers.common['X-Telegram-Init-Data'] = initData;
    console.log('‚úÖ –ó–∞–≥–æ–ª–æ–≤–∫–∏ –∞—É—Ç–µ–Ω—Ç–∏—Ñ–∏–∫–∞—Ü–∏–∏ —É—Å—Ç–∞–Ω–æ–≤–ª–µ–Ω—ã:');
